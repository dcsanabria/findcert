@{
    Layout = null;
}

<!doctype html>
<html>
<head>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>Qlik Sense Mashup</title>
    <meta charset="utf-8">
    <meta name="HandheldFriendly" content="True">
    <meta name="MobileOptimized" content="320">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0, user-scalable=no">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <meta http-equiv="cleartype" content="on">
    <script src="https://qlikdev.flipgroup.com.au/resources/assets/external/requirejs/require.js"></script>
    <!-- Bootstrap core CSS -->

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://qlikdev.flipgroup.com.au/resources/autogenerated/qlik-styles.css">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.6-rc.0/css/select2.min.css" rel="stylesheet">
    <!-- jQuery library -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <!-- Latest compiled JavaScript -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>

    <!-- Latest compiled and minified CSS -->
    <!-- Latest compiled and minified JavaScript -->

    <style>
        /**
        * set padding to make room for bootstrap menu
        *
        */
        body {
            padding: 10px;
        }

        /*
        * need to set the height for objects
        * set top padding to make room for selection toolbar
        */
        div.qvobject, div.qvplaceholder {
            padding: 10px 10px 10px 10px;
            height: 400px;
        }

        div.topQVObjectRow {
            padding: 10px 10px 10px 10px;
            height: 200px;
        }

        .qcmd {
            cursor: pointer;
        }

        #popup {
            background-color: #7f8c8d;
            color: #FFFFFF;
            position: fixed;
            padding: 10px;
            margin: 10px;
            bottom: 0;
            right: 0;
            display: none;
            border-radius: 5px;
        }

        /*
         * Overrides Bootstrap navbar z-index to avoid conflict with selections bar
         */
        .navbar-fixed-top {
            position: relative;
            z-index: 100;
        }

        .divCarrier select.badge {
            position: absolute;
            top: -18px;
            left: 18px;
        }
    </style>
</head>
<body role="document" style="overflow: auto">
    <!-- Fixed navbar -->
    <div class="navbar navbar-inverse navbar-fixed-top" role="navigation">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <span class="navbar-brand" data-toggle="tooltip" data-placement="right" id="title">Qlik Sense</span>

            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav pull-right">
                    <li>
                        <a class="qcmd" data-qcmd="clearAll">ClearAll</a>
                    </li>
                    <li>
                        <a class="qcmd" data-qcmd="back">Back</a>
                    </li>
                    <li>
                        <a class="qcmd" data-qcmd="forward">Forward</a>
                    </li>
                    <li class="dropdown qcmd">
                        <a class="dropdown-toggle" data-toggle="dropdown">Bookmarks <b class="caret"></b></a>
                        <ul id="qbmlist" class="dropdown-menu dropdown-menu-right"></ul>
                    </li>
                    <li class="dropdown qcmd">
                        <a class="dropdown-toggle" data-toggle="dropdown">More<b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li>
                                <a class="qcmd" data-qcmd="lockAll">Lock</a>
                            </li>
                            <li>
                                <a class="qcmd" data-qcmd="unlockAll">UnLock</a>
                            </li>
                        </ul>

                    </li>
                </ul>
            </div>
            <div>
                <button data-toggle="collapse" data-target="#divFilter">Show Filter</button>
            </div>
            <!--/.nav-collapse -->
        </div>
    </div>

    <div class="row collapse" id="divFilter">
        <div class="col-sm-1" id="QV01">
            <label for="lstReportingYear">Year</label>
            <select class="form-control  lstReportingYear" id="lstReportingYear" name="reportingyears[]" style="width: 100%;" multiple="multiple" data-live-search="true" data-selected-text-format="count > 2" title="Choose Year"></select>  <span id="yearBadge" class="badge badge-success pull-right"></span>
        </div>
        <div class="col-sm-2" id="QV02">
            <label for="lstReportingMonth">Month</label>
            <select class="form-control  lstReportingMonth" id="lstReportingMonth" name="reportingmonths[]" style="width: 100%;" multiple="multiple" data-live-search="true" data-selected-text-format="count > 2" title="Choose Month"></select>  <span id="monthBadge" class="badge badge-success pull-right"></span>
        </div>
        <div class="col-sm-3" id="QV03">
            <label for="lstCustomer">Customer</label>
            <select class="form-control  lstCustomer" id="lstCustomer" name="customers[]" style="width: 100%;" multiple="multiple" data-live-search="true" data-selected-text-format="count > 2" title="Choose Customer"></select>  <span id="customerBadge" class="badge badge-success pull-right"></span>
        </div>
        <div class="col-sm-3" id="QV04">
            <label for="lstCarrier">Carrier</label>
            <select class="form-control  lstCarrier" id="lstCarrier" name="carriers[]" style="width: 100%;" multiple="multiple" data-live-search="true" data-selected-text-format="count > 2" title="Choose Carrier"></select>
            <span id="carrierBadge" class="badge badge-success pull-right"></span>
        </div>

        <div class="col-sm-1" id="QV05">
            <label for="lstIsMonitored">Monitored?</label>
            <select class="form-control  lstIsMonitored" id="lstIsMonitored" name="monitored[]" style="width: 100%;" multiple="multiple" data-live-search="true" data-selected-text-format="count > 2" title="Choose Monitored">
                <option value=""></option>
            </select>  <span id="isMonitoredBadge" class="badge badge-success pull-right"></span>
        </div>
        <div class="col-sm-1" id="QV06">
            <label for="lstSenderState">Sender_State</label>
            <select class="form-control  lstSenderState" id="lstSenderState" name="senderStates[]" style="width: 100%;" multiple="multiple" data-live-search="true" data-selected-text-format="count > 2" title="Choose Sender State">
                <option value="">Select Sender State</option>
            </select>  <span id="senderStateBadge" class="badge badge-success pull-right"></span>
        </div>
        <div class="col-sm-1" id="QV07">
            <label for="lstReceiverState">Receiver_State</label>
            <select class="form-control  lstReceiverState" id="lstReceiverState" name="receiverStates[]" style="width: 100%;" multiple="multiple" data-live-search="true" data-selected-text-format="count > 2" title="Choose Receiver State">
                <option value="">Select Receiver State</option>
            </select>  <span id="receiverStateBadge" class="badge badge-success pull-right"></span>
        </div>
    </div>
    <div class="" id="main" role="main">

        <div class="row">
            <div class="col-sm-4 topQVObjectRow" id="QV08"></div>
            <div class="col-sm-2 topQVObjectRow" id="QV09"></div>
            <div class="col-sm-2 topQVObjectRow" id="QV10"></div>
            <div class="col-sm-2 topQVObjectRow" id="QV11"></div>
            <div class="col-sm-2 topQVObjectRow" id="QV12"></div>

        </div>

        <div class="row">
            <div class="col-sm-6 qvobject" id="QV13"></div>
            <div class="col-sm-6 qvobject" id="QV14"></div>
        </div>

        <!-- add more rows here if you want more visualizations -->
    </div>

    <!-- Bootstrap Modals -->
    <div class="modal " id="createBmModal">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">×</span></button>
                    <h4 class="modal-title">Create bookmark</h4>
                </div>
                <div class="modal-body">
                    <form>
                        <label for="bmtitle" class="control-label">Title:</label>
                        <input type="text" class="form-control" id="bmtitle">
                        <label for="bmdesc" class="control-label">Description:</label>
                        <input type="text" class="form-control" id="bmdesc">
                    </form>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                    <button type="button" data-qcmd="createBm" class="qcmd btn btn-primary">Create</button>
                </div>
            </div>
        </div>
    </div>
    <!--Error Popup-->
    <div id="popup">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close" id="closePopup"><span aria-hidden="true">×</span></button>

        <p id="popupText">
        </p>
    </div>
    <script>
        require(['https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.6-rc.0/js/select2.min.js']);
    </script>
    <script type="text/javascript" src="~/Scripts/QlikScript.js"></script>


</body>
</html>